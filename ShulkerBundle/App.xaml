<Application x:Class="ShulkerBundle.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ShulkerBundle"
             xmlns:converters="clr-namespace:ShulkerBundle.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Themes/WorldCard.xaml"/>
                <ResourceDictionary Source="./Themes/PackCard.xaml" />
                <ResourceDictionary Source="./Themes/StructureCard.xaml" />
                <ResourceDictionary Source="./Themes/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
